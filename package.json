{
  "name": "@teriologia/sonicdb",
  "version": "0.2.3",
  "description": "Stop treating your queries like a 'Labyrinth Zone' water level. SonicDB gets you in and out instantly.",
  "main": "./dist/cjs/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/mjs/index.d.ts",
        "default": "./dist/mjs/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "clean": "rimraf dist",
    "build:cjs": "tsc -p tsconfig.cjs.json",
    "build:mjs": "tsc -p tsconfig.mjs.json",
    "build": "npm run clean && npm run build:cjs && npm run build:mjs"
  },
  "keywords": [
    "database",
    "in-memory",
    "fast",
    "index",
    "mongoose",
    "sonic",
    "sonicdb",
    "db",
    "jsdb",
    "teriologia",
    "teriologiadb",
    "@teriologia/sonicdb",
    "@teriologia"
  ],
  "author": "Teriologia <Mehmetserdartekin@gmail.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/teriologia/SonicDB.git"
  },
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/teriologia/SonicDB/issues"
  },
  "homepage": "https://github.com/teriologia/SonicDB#readme",
  "devDependencies": {
    "@types/node": "^24.9.2",
    "rimraf": "^6.1.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  }
}
